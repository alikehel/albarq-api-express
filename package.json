{
    "name": "albarq-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start:dev": "nodemon ./src/index.ts",
        "start": "npm run swagger-autogen && npx prisma migrate deploy && npm run build && node build/index.js",
        "build": "npm run clean && npx tsc",
        "clean": "rimraf build/",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
        "lint": "eslint . --ext .ts",
        "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build && 'node ./build/index.js'",
        "swagger-autogen": "ts-node ./src/swagger/swagger.ts",
        "migrate:reset": "set NODE_ENV=dev& npx prisma db push && npx prisma generate && npx prisma migrate dev && npx prisma migrate reset",
        "seed": "set NODE_ENV=dev&  npx prisma generate && npx prisma db push && npx prisma db seed",
        "prisma:studio": "npx prisma studio"
    },
    "prisma": {
        "seed": "ts-node src/database/seed.ts",
        "schema": "src/database/schema.prisma"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.294.0",
        "@aws-sdk/s3-request-presigner": "^3.294.0",
        "@prisma/client": "^5.4.2",
        "axios": "^1.4.0",
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "openai": "^4.12.1",
        "swagger-themes": "^1.2.28",
        "swagger-ui-express": "^5.0.0",
        "uuid": "^9.0.0",
        "zod": "^3.21.4",
        "zod_utilz": "^0.7.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/jsonwebtoken": "^9.0.3",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.8.5",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "prisma": "^5.4.2",
        "rimraf": "^5.0.5",
        "swagger-autogen": "^2.23.1",
        "ts-node": "^10.9.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.0.4"
    }
}
